{"Columns":[{"$type":"Forguncy.Model.Tables.BindingColumn, ServerDesignerCommon","DatabaseColumnType":"System.String","MaxLength":-1,"Name":"公司名称","ColumnType":"System.String, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"},{"$type":"Forguncy.Model.Tables.BindingColumn, ServerDesignerCommon","DatabaseColumnType":"System.String","MaxLength":-1,"Name":"产品名称","ColumnType":"System.String, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"},{"$type":"Forguncy.Model.Tables.BindingColumn, ServerDesignerCommon","DatabaseColumnType":"System.Object","MaxLength":-1,"Name":"销售量","ColumnType":"System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"},{"$type":"Forguncy.Model.Tables.BindingColumn, ServerDesignerCommon","DatabaseColumnType":"System.Object","MaxLength":-1,"Name":"销售金额","ColumnType":"System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"}],"BuiltInViewInfo":{"ViewName":"SalesBySupplier","CreateViewSql":"SELECT\r\n    供应商.公司名称,\r\n    产品.产品名称,\r\n    SUM(订单明细.数量) AS 销售量,\r\n    SUM(订单明细.单价 * 订单明细.数量) AS 销售金额\r\nFROM\r\n    订单明细\r\n    INNER JOIN (\r\n        产品\r\n        INNER JOIN 供应商 ON 产品.供应商ID = 供应商.供应商ID\r\n    ) ON 订单明细.产品ID = 产品.产品ID\r\nGROUP BY\r\n    供应商.公司名称,\r\n    产品.产品ID,\r\n    产品.产品名称\r\nORDER BY\r\n    供应商.公司名称;"},"Name":"SalesBySupplier","Relations":[]}