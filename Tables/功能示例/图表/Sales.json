{"Columns":[{"$type":"Forguncy.Model.Tables.BindingColumn, ServerDesignerCommon","DatabaseColumnType":"System.String","MaxLength":-1,"Name":"产品名称","ColumnType":"System.String, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"},{"$type":"Forguncy.Model.Tables.BindingColumn, ServerDesignerCommon","DatabaseColumnType":"System.String","MaxLength":-1,"Name":"货主地区","ColumnType":"System.String, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"},{"$type":"Forguncy.Model.Tables.BindingColumn, ServerDesignerCommon","DatabaseColumnType":"System.Object","MaxLength":-1,"Name":"销售量","ColumnType":"System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"}],"BuiltInViewInfo":{"ViewName":"Sales","CreateViewSql":"SELECT 产品.产品名称,订单.货主地区,SUM(订单明细.数量) AS 销售量 \r\nFROM (( 订单 \n INNER JOIN 订单明细 ON 订单.订单ID = 订单明细.订单ID)\n INNER JOIN 产品 ON 订单明细.产品ID = 产品.产品ID)\r\n\n WHERE  strftime('%Y',订单.订购日期) = '2011'\n GROUP BY 产品.产品名称,订单.货主地区\n ORDER BY SUM(订单明细.数量) desc\nlimit 0,30"},"Name":"Sales","Relations":[]}